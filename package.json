{
  "name": "libsql-search",
  "version": "0.1.0",
  "description": "Semantic search for static sites using libSQL/Turso with multi-provider embeddings",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "libsql",
    "turso",
    "vector-search",
    "semantic-search",
    "embeddings",
    "static-sites",
    "markdown",
    "astro",
    "nextjs"
  ],
  "author": "llbbl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/llbbl/libsql-search.git"
  },
  "bugs": {
    "url": "https://github.com/llbbl/libsql-search/issues"
  },
  "homepage": "https://github.com/llbbl/libsql-search#readme",
  "peerDependencies": {
    "@libsql/client": "^0.15.0"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "gray-matter": "^4.0.3"
  },
  "optionalDependencies": {
    "@google/generative-ai": "^0.24.1"
  },
  "devDependencies": {
    "@libsql/client": "^0.15.15",
    "@types/node": "^20.19.21",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
